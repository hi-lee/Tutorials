import javax.swing.*;
import java.awt.*;

public class EX09_01 extends JApplet{
	public void init() {
		setSize(400,300);
	}
	public void paint(Graphics g) {
		g.setColor(Color.red);
		g.fillOval(50, 50, 100, 100);
		g.setColor(Color.orange);
		g.fillOval(72, 72, 55, 55);
		g.setColor(Color.green);
		g.fillRect(100, 70, 10, 50);
	}
}
